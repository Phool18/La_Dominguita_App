<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#20000000"/>
        </shape>
    </item>
    <item android:left="2dp" android:right="2dp" android:top="1dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#4E4D4D"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item android:left="4dp" android:right="4dp" android:top="2dp" android:bottom="4dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:startColor="#FFCCBC"
                android:endColor="#35345C"
                android:type="linear"/>
            <corners android:radius="8dp"/>
        </shape>
    </item>
</layer-list>
